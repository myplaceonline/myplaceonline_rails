<%= attribute_table_row(t("myplaceonline.documents.document_name"), obj.document_name) %>
<%= attribute_table_row_date(t("myplaceonline.documents.document_date"), obj.document_date) %>
<%= data_row(heading: t("myplaceonline.documents.coauthors"), content: obj.coauthors) %>
<%= attribute_table_row(t("myplaceonline.documents.document_category"), obj.document_category) %>
<%= attribute_table_row_boolean(t("myplaceonline.documents.important"), obj.important) %>
<%= attribute_table_row_markdown(t("myplaceonline.general.notes"), obj.notes) %>
<%=
  render partial: "myplaceonline/pictures", locals: {
    pics: obj.document_files,
    placeholder: "myplaceonline.documents.file"
  }
%>

