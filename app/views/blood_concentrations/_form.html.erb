<%= myp_text_field(f, :concentration_name, "myplaceonline.blood_concentrations.concentration_name", obj.concentration_name, autofocus) %>
<%= myp_select(f, :concentration_type, "myplaceonline.blood_concentrations.concentration_type", Myp.translate_options(Myp::LIQUID_CONCENTRATIONS, sort: true), obj.concentration_type) %>
<%= myp_decimal_field(f, :concentration_minimum, "myplaceonline.blood_concentrations.concentration_minimum", obj.concentration_minimum, false, nil, "0.001") %>
<%= myp_decimal_field(f, :concentration_maximum, "myplaceonline.blood_concentrations.concentration_maximum", obj.concentration_maximum, false, nil, "0.001") %>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_TEXT_AREA,
    name: :notes,
    value: obj.notes,
    placeholder: "myplaceonline.general.notes",
    collapsed: obj.notes.blank?,
    placeholder_prefix: I18n.t("myplaceonline.category.blood_concentrations").singularize,
  )
%>
