<%=
  input_field(
    form: f,
    type: Myp::FIELD_TEXT,
    name: :dietary_requirement_name,
    value: obj.dietary_requirement_name,
    placeholder: "myplaceonline.dietary_requirements.dietary_requirement_name",
    autofocus: autofocus,
    remote_autocomplete_model: FoodNutrientInformation,
    remote_autocomplete_name: "nutrient_name",
    remote_autocomplete_identity: User::SUPER_USER_IDENTITY_ID
  )
%>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_DECIMAL,
    name: :dietary_requirement_amount,
    value: obj.dietary_requirement_amount,
    placeholder: "myplaceonline.dietary_requirements.dietary_requirement_amount",
  )
%>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_SELECT,
    name: :dietary_requirement_type,
    value: obj.dietary_requirement_type,
    placeholder: "myplaceonline.dietary_requirements.dietary_requirement_type",
    select_options: Nutrient::MEASUREMENTS,
  )
%>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_SELECT,
    name: :dietary_requirement_context,
    value: obj.dietary_requirement_context,
    placeholder: "myplaceonline.dietary_requirements.dietary_requirement_context",
    select_options: DietaryRequirement::CONTEXTS,
  )
%>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_TEXT_AREA,
    name: :notes,
    value: obj.notes,
    placeholder: "myplaceonline.general.notes",
    collapsed: obj.notes.blank?,
    placeholder_prefix: Myp.instance_to_category_human_readable(obj).singularize,
  )
%>
