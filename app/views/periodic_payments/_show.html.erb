<%= attribute_table_row(t("myplaceonline.periodic_payments.periodic_payment_name"), obj.periodic_payment_name) %>
<%= attribute_table_row_date(t("myplaceonline.periodic_payments.started"), obj.started) %>
<%= attribute_table_row_date(t("myplaceonline.periodic_payments.ended"), obj.ended) %>
<%= attribute_table_row(t("myplaceonline.periodic_payments.payment_amount"), obj.payment_amount) %>
<%= attribute_table_row_select(t("myplaceonline.periodic_payments.date_period"), obj.date_period, Myp::PERIODS) %>
<%= attribute_table_row_boolean(t("myplaceonline.periodic_payments.suppress_reminder"), obj.suppress_reminder) %>
<%= attribute_table_row_date(t("myplaceonline.periodic_payments.next_payment"), obj.next_payment) %>
<%= attribute_table_row_markdown(t("myplaceonline.general.notes"), obj.notes) %>
<%= attribute_table_row_reference(t("myplaceonline.periodic_payments.password"), "password_path", obj.password) %>
<% obj.periodic_payment_instances.each do |x| %>
  <%= table_row_heading(t("myplaceonline.periodic_payments.instance")) %>
  <%= render partial: "periodic_payment_instances/show", locals: { obj: x } %>
<% end %>
<%= data_row(heading: t("myplaceonline.identity_files.file"), content: obj.periodic_payment_files) %>
