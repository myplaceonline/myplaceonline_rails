<%= myp_datetime_field(f, :exercise_start, "myplaceonline.exercises.exercise_start", obj.exercise_start) %>
<%= myp_datetime_field(f, :exercise_end, "myplaceonline.exercises.exercise_end", obj.exercise_end) %>
<%= myp_text_field(f, :exercise_activity, "myplaceonline.exercises.exercise_activity", obj.exercise_activity) %>
<%= myp_integer_field(f, :situps, "myplaceonline.exercises.situps", obj.situps) %>
<%= myp_integer_field(f, :pushups, "myplaceonline.exercises.pushups", obj.pushups) %>
<%= myp_integer_field(f, :cardio_time, "myplaceonline.exercises.cardio_time", obj.cardio_time) %>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_TEXT_AREA,
    name: :notes,
    value: obj.notes,
    placeholder: "myplaceonline.general.notes",
    collapsed: obj.notes.blank?,
    placeholder_prefix: Myp.instance_to_category_human_readable(obj).singularize,
  )
%>
