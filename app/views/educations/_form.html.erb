<%= myp_text_field(f, :education_name, "myplaceonline.educations.education_name", obj.education_name, autofocus) %>
<%= myp_date_field(f, :education_start, "myplaceonline.educations.education_start", obj.education_start) %>
<%= myp_date_field(f, :education_end, "myplaceonline.educations.education_end", obj.education_end) %>
<%= myp_select(f, :degree_type, "myplaceonline.educations.degree_type", Myp.translate_options(Education::EDUCATION_TYPES), obj.degree_type) %>
<%= myp_text_field(f, :degree_name, "myplaceonline.educations.degree_name", obj.degree_name) %>
<%= myp_decimal_field(f, :gpa, "myplaceonline.educations.gpa", obj.gpa) %>
<%= myp_text_field(f, :student_id, "myplaceonline.educations.student_id", obj.student_id) %>
<%= myp_check_box(f, :is_graduated, "myplaceonline.educations.graduated") %>
<%= myp_text_area_markdown(f, :notes, "myplaceonline.general.notes", obj.notes) %>
<%=
  render partial: 'myplaceonline/select_or_create', locals: {
    f: f,
    name: :location,
    existingobj: obj.location,
    heading: t("myplaceonline.educations.location"),
    objform: "locations/form",
    existingobjurl: "location_path",
    searchurl: "/locations.json?perpage=0",
    edit: edit,
    autofocus: false,
    collapsed: obj.location.nil?
  }
%>
<%=
  render partial: "myplaceonline/pictures_form", locals: {
    f: f,
    obj: obj,
    position_field: :position,
    pictures_field: :education_files,
    item_placeholder: "myplaceonline.educations.file",
    heading: "myplaceonline.educations.files",
    addbutton: "myplaceonline.educations.add_file",
    addbutton_multi: "myplaceonline.educations.add_files",
    deletebutton: "myplaceonline.educations.delete_file"
  }
%>
