  <h3><%= t("myplaceonline.website_domains.hosted_details") %></h3>
  <%=
    input_field(
      form: f,
      type: Myp::FIELD_TEXT,
      name: :meta_description,
      value: obj.meta_description,
      placeholder: "myplaceonline.website_domains.meta_description",
    )
