      position_field: :position,
      itemname: t("myplaceonline.website_domains.website_domain_myplet"),
      formjson: [
        {
          type: :renderpartial,
          partial: "myplaceonline/select_or_create",
          heading: t("myplaceonline.website_domains.website_domain_myplet"),
