<% if edit %>
  <%= myp_text_field(f, :file_file_name, "myplaceonline.files.file_file_name", obj.file_file_name) %>
<% end %>
<%= myp_file_field(f, :file, "myplaceonline.files.file", obj.file, autofocus, useprogress: edit ? false : true) %>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_TEXT_AREA,
    name: :notes,
    value: obj.notes,
    placeholder: "myplaceonline.general.notes",
    collapsed: obj.notes.blank?,
    placeholder_prefix: Myp.instance_to_category_human_readable(obj).singularize,
  )
%>
<%= myp_hidden_field(f, :_updatetype, AllowExistingConcern::UPDATE_TYPE_COMBINE.to_s) %>
<%= f.fields_for(:folder, obj.folder) do |folder_form| %>
<% end %>
