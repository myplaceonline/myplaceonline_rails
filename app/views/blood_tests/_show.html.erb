<%= attribute_table_row_datetime(t("myplaceonline.blood_tests.test_time"), obj.test_time) %>
<%= attribute_table_row_datetime(t("myplaceonline.blood_tests.fast_started"), obj.fast_started) %>
<%= attribute_table_row(t("myplaceonline.blood_tests.preceding_changes"), obj.preceding_changes) %>
<%= attribute_table_row_markdown(t("myplaceonline.general.notes"), obj.notes) %>
<% obj.blood_test_results.each do |wrapper| %>
  <%= table_row_heading(t("myplaceonline.blood_tests.result")) %>
  <%= attribute_table_row_reference(t("myplaceonline.blood_concentrations.concentration_name"), "blood_concentration_path", wrapper.blood_concentration) %>
  <%= attribute_table_row_liquid_concentration(t("myplaceonline.blood_tests.concentration"), wrapper.concentration, wrapper.blood_concentration.concentration_type, false) %>
  <%= attribute_table_row_select(t("myplaceonline.blood_tests.flag"), wrapper.flag, BloodTestResult::FLAGS) %>
  <%= attribute_table_row_liquid_concentration(t("myplaceonline.blood_concentrations.concentration_minimum"), wrapper.blood_concentration.concentration_minimum, wrapper.blood_concentration.concentration_type, false) %>
  <%= attribute_table_row_liquid_concentration(t("myplaceonline.blood_concentrations.concentration_maximum"), wrapper.blood_concentration.concentration_maximum, wrapper.blood_concentration.concentration_type, false) %>
<% end %>
<%=
  render partial: "myplaceonline/pictures", locals: {
    pics: @obj.blood_test_files,
    placeholder: "myplaceonline.blood_tests.file"
  }
%>
<%= attribute_table_row_reference(t("myplaceonline.blood_tests.location"), "location_path", obj.location) %>
<%= attribute_table_row_reference(t("myplaceonline.blood_tests.doctor"), "doctor_path", obj.doctor) %>
