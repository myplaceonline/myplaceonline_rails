<%=
  input_field(
    form: f,
    type: Myp::FIELD_TEXT,
    name: :license_name,
    value: obj.license_name,
    placeholder: "myplaceonline.software_licenses.license_name",
    autofocus: autofocus
  )
%>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_TEXT_AREA,
    name: :license_key,
    value: obj.license_key,
    placeholder: "myplaceonline.software_licenses.license_key",
    collapsed: false
  )
%>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_DATE,
    name: :license_purchase_date,
    value: obj.license_purchase_date,
    placeholder: "myplaceonline.software_licenses.license_purchase_date"
  )
%>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_DECIMAL,
    name: :license_value,
    value: obj.license_value,
    placeholder: "myplaceonline.software_licenses.license_value"
  )
%>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_TEXT_AREA,
    name: :notes,
    value: obj.notes,
    placeholder: "myplaceonline.general.notes",
    collapsed: obj.notes.blank?,
    placeholder_prefix: Myp.instance_to_category_human_readable(obj).singularize,
  )
%>
<%=
  render partial: "myplaceonline/pictures_form", locals: {
    f: f,
    obj: obj,
    position_field: :position,
    pictures_field: :software_license_files,
    item_placeholder: "myplaceonline.identity_files.file",
    heading: "myplaceonline.identity_files.files",
    addbutton: "myplaceonline.identity_files.add_file",
    addbutton_multi: "myplaceonline.identity_files.add_files",
    deletebutton: "myplaceonline.identity_files.delete_file"
  }
%>
