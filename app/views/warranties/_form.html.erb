<%= myp_text_field(f, :warranty_name, "myplaceonline.warranties.warranty_name", obj.warranty_name, autofocus) %>
<%= myp_date_field(f, :warranty_start, "myplaceonline.warranties.warranty_start", obj.warranty_start) %>
<%= myp_date_field(f, :warranty_end, "myplaceonline.warranties.warranty_end", obj.warranty_end) %>
<%= myp_text_field(f, :warranty_condition, "myplaceonline.warranties.warranty_condition", obj.warranty_condition) %>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_TEXT_AREA,
    name: :notes,
    value: obj.notes,
    placeholder: "myplaceonline.general.notes",
    collapsed: obj.notes.blank?,
    placeholder_prefix: Myp.instance_to_category_human_readable(obj).singularize,
  )
%>
