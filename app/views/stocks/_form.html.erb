<%=
  render partial: "myplaceonline/select_or_create", locals: {
    f: f,
    name: :company,
    existingobj: obj.company,
    heading: t("myplaceonline.stocks.company"),
    objform: "companies/form",
    existingobjurl: "company_path",
    searchurl: "/companies.json?perpage=0",
    edit: edit,
    autofocus: false,
    expanded_if_nil: true
  }
%>
<%= myp_integer_field(f, :num_shares, "myplaceonline.stocks.num_shares", obj.num_shares) %>
<%= myp_date_field(f, :vest_date, "myplaceonline.stocks.vest_date", obj.vest_date) %>
<%=
  render partial: "myplaceonline/select_or_create", locals: {
    f: f,
    name: :password,
    existingobj: obj.password,
    heading: t("myplaceonline.stocks.password"),
    objform: "passwords/form",
    existingobjurl: "password_path",
    searchurl: "/passwords.json?perpage=0",
    edit: edit,
    autofocus: false
  }
%>
<%= myp_text_area_markdown(f, :notes, "myplaceonline.stocks.notes", obj.notes) %>
<%=
  render partial: "myplaceonline/pictures_form", locals: {
    f: f,
    obj: obj,
    position_field: :position,
    pictures_field: :stock_files,
    item_placeholder: "myplaceonline.identity_files.file",
    heading: "myplaceonline.identity_files.files",
    addbutton: "myplaceonline.identity_files.add_file",
    addbutton_multi: "myplaceonline.identity_files.add_files",
    deletebutton: "myplaceonline.identity_files.delete_file"
  }
%>
