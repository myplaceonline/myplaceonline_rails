<%= myp_text_field(f, :name, "myplaceonline.bank_accounts.name", obj.name, autofocus) %>
<%= myp_text_field(f, :account_number, "myplaceonline.bank_accounts.account_number", obj.account_number) %>
<%= myp_text_field(f, :routing_number, "myplaceonline.bank_accounts.routing_number", obj.routing_number) %>
<%= myp_text_field(f, :pin, "myplaceonline.bank_accounts.pin", obj.pin) %>
<%= render(partial: "myplaceonline/encrypt_fields", locals: { f: f }) %>
<%=
  render partial: "myplaceonline/select_or_create", locals: {
    f: f,
    name: :password,
    existingobj: obj.password,
    heading: t("myplaceonline.bank_accounts.password"),
    objform: "passwords/form",
    existingobjurl: "password_path",
    searchurl: "/passwords.json?perpage=0",
    edit: edit,
    autofocus: false
  }
%>
<%=
  render partial: "myplaceonline/select_or_create", locals: {
    f: f,
    name: :home_address,
    existingobj: obj.home_address,
    heading: t("myplaceonline.bank_accounts.home_address"),
    objform: "locations/form",
    existingobjurl: "location_path",
    searchurl: "/locations.json?perpage=0",
    edit: edit,
    autofocus: false
  }
%>
<%=
  render partial: "myplaceonline/select_or_create", locals: {
    f: f,
    name: :company,
    existingobj: obj.company,
    heading: t("myplaceonline.bank_accounts.company"),
    objform: "companies/form",
    existingobjurl: "company_path",
    searchurl: "/companies.json?perpage=0",
    edit: edit,
    autofocus: false
  }
%>
<%=
  render partial: "myplaceonline/pictures_form", locals: {
    f: f,
    obj: obj,
    position_field: :position,
    pictures_field: :bank_account_files,
    item_placeholder: "myplaceonline.identity_files.file",
    heading: "myplaceonline.identity_files.files",
    addbutton: "myplaceonline.identity_files.add_file",
    addbutton_multi: "myplaceonline.identity_files.add_files",
    deletebutton: "myplaceonline.identity_files.delete_file"
  }
%>
