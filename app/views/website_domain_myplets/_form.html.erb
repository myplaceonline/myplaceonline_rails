<%=
  input_field(
    form: f,
    type: Myp::FIELD_TEXT,
    name: :title,
    value: obj.title,
    placeholder: "myplaceonline.website_domain_myplets.title",
    autofocus: autofocus
  )
%>
<%=
  render partial: "myplaceonline/select_or_create", locals: {
    f: f,
    name: :category,
    existingobj: obj.category,
    heading: t("myplaceonline.website_domain_myplets.category"),
    existingobjurl: "category_path",
    searchurl: "/categories.json?perpage=0",
    edit: edit,
    autofocus: false,
    allow_new: false,
    model: Category,
    expanded_if_nil: true,
    existingcollapsed: false,
  }
%>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_SELECT,
    name: :border_type,
    value: obj.border_type,
    placeholder: "myplaceonline.website_domain_myplets.border_type",
    select_options: Myplet::BORDER_TYPES,
  )
%>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_BOOLEAN,
    name: :singleton,
    value: obj.singleton,
    placeholder: "myplaceonline.website_domain_myplets.singleton"
  )
%>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_BOOLEAN,
    name: :emulate_guest,
    value: obj.emulate_guest,
    placeholder: "myplaceonline.website_domain_myplets.emulate_guest"
  )
%>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_TEXT,
    name: :action,
    value: obj.action,
    placeholder: "myplaceonline.website_domain_myplets.action",
  )
%>
<%=
  render(layout: "myplaceonline/childproperties", locals: {
    f: f,
    heading: t("myplaceonline.website_domain_myplets.website_domain_myplet_parameters"),
    childpropertiesname: :website_domain_myplet_parameters,
    childproperties: obj.website_domain_myplet_parameters,
    deletebutton: t("myplaceonline.website_domain_myplets.delete_website_domain_myplet_parameter"),
    addbutton: t("myplaceonline.website_domain_myplets.add_website_domain_myplet_parameter"),
    expanded: obj.website_domain_myplet_parameters.length > 0,
    formjson: [
      {
        type: :renderpartial,
        partial: "myplaceonline/select_or_create",
        heading: t("myplaceonline.website_domain_myplets.website_domain_myplet_parameter"),
        objform: "website_domain_myplet_parameters/form",
        only_new: true,
        edit: false,
        autofocus: true
      },
    ]
  }) do |child_fields, childproperty|
%>
  <%= render partial: "website_domain_myplet_parameters/form", locals: { obj: childproperty || WebsiteDomainMypletParameter.build, edit: edit, autofocus: false, f: child_fields } %>
<% end %>
