<%= myp_date_field(f, :input_date, "myplaceonline.retirement_plan_amounts.input_date", obj.input_date) %>
<%= myp_decimal_field(f, :amount, "myplaceonline.retirement_plan_amounts.amount", obj.amount, autofocus) %>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_TEXT_AREA,
    name: :notes,
    value: obj.notes,
    placeholder: "myplaceonline.general.notes",
    collapsed: obj.notes.blank?,
    placeholder_prefix: Myp.instance_to_category_human_readable(obj).singularize,
  )
%>
<%=
  render partial: "myplaceonline/pictures_form", locals: {
    f: f,
    obj: obj,
    position_field: :position,
    pictures_field: :retirement_plan_amount_files,
    item_placeholder: "myplaceonline.retirement_plan_amounts.file",
    heading: "myplaceonline.retirement_plan_amounts.files",
    addbutton: "myplaceonline.retirement_plan_amounts.add_file",
    addbutton_multi: "myplaceonline.retirement_plan_amounts.add_files",
    deletebutton: "myplaceonline.retirement_plan_amounts.delete_file"
  }
%>
