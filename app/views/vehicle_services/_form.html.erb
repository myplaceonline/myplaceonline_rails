<%= myp_text_field(f, :short_description, "myplaceonline.vehicle_service.short_description", obj.short_description, autofocus) %>
<%= myp_date_field(f, :date_serviced, "myplaceonline.vehicle_service.date_serviced", obj.date_serviced) %>
<%= myp_date_field(f, :date_due, "myplaceonline.vehicle_service.date_due", obj.date_due) %>
<%= myp_text_field(f, :service_location, "myplaceonline.vehicle_service.service_location", obj.service_location, remote_autocomplete_model: obj.class, remote_autocomplete_all: true) %>
<%= myp_text_field(f, :cost, "myplaceonline.vehicle_service.cost", obj.cost) %>
<%= myp_text_field(f, :miles, "myplaceonline.vehicle_service.miles", obj.miles) %>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_TEXT_AREA,
    name: :notes,
    value: obj.notes,
    placeholder: "myplaceonline.general.notes",
    collapsed: obj.notes.blank?,
    placeholder_prefix: Myp.instance_to_category_human_readable(obj).singularize,
  )
%>
<%=
  render partial: "myplaceonline/pictures_form", locals: {
    f: f,
    obj: obj,
    position_field: :position,
    pictures_field: :vehicle_service_files,
    item_placeholder: "myplaceonline.vehicle_services.file",
    heading: "myplaceonline.vehicle_services.files",
    addbutton: "myplaceonline.vehicle_services.add_file",
    addbutton_multi: "myplaceonline.vehicle_services.add_files",
    deletebutton: "myplaceonline.vehicle_services.delete_file"
  }
%>
