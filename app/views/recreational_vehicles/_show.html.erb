<% obj.recreational_vehicle_loans.each do |loanwrapper| %>
  <%= table_row_heading(t("myplaceonline.loans.loan")) %>
  <%= attribute_table_row(t("myplaceonline.loans.lender"), loanwrapper.loan.lender) %>
  <%= attribute_table_row(t("myplaceonline.loans.amount"), loanwrapper.loan.amount) %>
  <%= attribute_table_row(t("myplaceonline.loans.monthly_payment"), loanwrapper.loan.monthly_payment) %>
  <%= attribute_table_row(t("myplaceonline.loans.start"), Myp.display_date(loanwrapper.loan.start, current_user)) %>
  <%= attribute_table_row(t("myplaceonline.loans.paid_off"), Myp.display_date(loanwrapper.loan.paid_off, current_user)) %>
<% end %>
<% obj.recreational_vehicle_insurances.each do |insurance| %>
  <%= table_row_heading(t("myplaceonline.recreational_vehicle_insurances.insurance")) %>
  <%= attribute_table_row(t("myplaceonline.recreational_vehicle_insurances.insurance_name"), insurance.insurance_name) %>
  <%= attribute_table_row_date(t("myplaceonline.recreational_vehicle_insurances.date_due"), insurance.started) %>
  <%= attribute_table_row_markdown(t("myplaceonline.general.notes"), insurance.notes) %>
  <%= attribute_table_row_reference(t("myplaceonline.recreational_vehicle_insurances.company"), "company_path", insurance.company) %>
  <%= attribute_table_row_reference(t("myplaceonline.recreational_vehicle_insurances.periodic_payment"), "periodic_payment_path", insurance.periodic_payment) %>
<% end %>
