<%= myp_text_field(f, :insurance_name, "myplaceonline.health_insurances.insurance_name", obj.insurance_name, autofocus) %>
<%= myp_text_field(f, :account_number, "myplaceonline.health_insurances.account_number", obj.account_number) %>
<%= myp_text_field(f, :group_number, "myplaceonline.health_insurances.group_number", obj.group_number) %>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_TEXT_AREA,
    name: :notes,
    value: obj.notes,
    placeholder: "myplaceonline.general.notes",
    collapsed: obj.notes.blank?,
    placeholder_prefix: Myp.instance_to_category_human_readable(obj).singularize,
  )
%>
<%=
  render partial: "myplaceonline/select_or_create", locals: {
    f: f,
    name: :insurance_company,
    existingobj: obj.insurance_company,
    heading: t("myplaceonline.health_insurances.insurance_company"),
    objform: "companies/form",
    existingobjurl: "company_path",
    searchurl: "/companies.json?perpage=0",
    edit: edit,
    autofocus: false
  }
%>
<%=
  render partial: "myplaceonline/select_or_create", locals: {
    f: f,
    name: :group_company,
    existingobj: obj.group_company,
    heading: t("myplaceonline.health_insurances.group_company"),
    objform: "companies/form",
    existingobjurl: "company_path",
    searchurl: "/companies.json?perpage=0",
    edit: edit,
    autofocus: false
  }
%>
<%=
  render partial: "myplaceonline/select_or_create", locals: {
    f: f,
    name: :periodic_payment,
    existingobj: obj.periodic_payment,
    heading: t("myplaceonline.health_insurances.periodic_payment"),
    objform: "periodic_payments/form",
    existingobjurl: "periodic_payment_path",
    searchurl: "/periodic_payments.json?perpage=0",
    edit: edit,
    autofocus: false
  }
%>
<%=
  render partial: "myplaceonline/select_or_create", locals: {
    f: f,
    name: :password,
    existingobj: obj.password,
    heading: t("myplaceonline.health_insurances.password"),
    objform: "passwords/form",
    existingobjurl: "password_path",
    searchurl: "/passwords.json?perpage=0",
    edit: edit,
    autofocus: false
  }
%>
<%=
  render partial: "myplaceonline/select_or_create", locals: {
    f: f,
    name: :doctor,
    existingobj: obj.doctor,
    heading: t("myplaceonline.health_insurances.doctor"),
    objform: "doctors/form",
    existingobjurl: "doctor_path",
    searchurl: "/doctors.json?perpage=0",
    edit: edit,
    autofocus: false
  }
%>
<%=
  render partial: "myplaceonline/pictures_form", locals: {
    f: f,
    obj: obj,
    position_field: :position,
    pictures_field: :health_insurance_files,
    item_placeholder: "myplaceonline.health_insurances.file",
    heading: "myplaceonline.health_insurances.files",
    addbutton: "myplaceonline.health_insurances.add_file",
    addbutton_multi: "myplaceonline.health_insurances.add_files",
    deletebutton: "myplaceonline.health_insurances.delete_file"
  }
%>
