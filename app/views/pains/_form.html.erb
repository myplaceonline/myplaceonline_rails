<%= myp_datetime_field(f, :pain_start_time, "myplaceonline.pains.pain_start_time", obj.pain_start_time) %>
<%= myp_datetime_field(f, :pain_end_time, "myplaceonline.pains.pain_end_time", obj.pain_end_time) %>
<%= myp_text_field(f, :pain_location, "myplaceonline.pains.pain_location", obj.pain_location) %>
<%= myp_select(f, :intensity, "myplaceonline.pains.intensity", Myp.translate_options(Myp::INTENSITIES), obj.intensity) %>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_TEXT_AREA,
    name: :notes,
    value: obj.notes,
    placeholder: "myplaceonline.general.notes",
    collapsed: obj.notes.blank?,
    placeholder_prefix: Myp.instance_to_category_human_readable(obj).singularize,
  )
%>
