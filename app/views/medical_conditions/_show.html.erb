<%= attribute_table_row(t("myplaceonline.medical_conditions.medical_condition_name"), obj.medical_condition_name) %>
<%= attribute_table_row_markdown(t("myplaceonline.medical_conditions.notes"), obj.notes) %>
<% obj.medical_condition_instances.each do |medical_condition_instance| %>
  <%= table_row_heading(t("myplaceonline.medical_conditions.instance")) %>
  <%= attribute_table_row_date(t("myplaceonline.medical_condition_instances.condition_start"), medical_condition_instance.condition_start) %>
  <%= attribute_table_row_date(t("myplaceonline.medical_condition_instances.condition_end"), medical_condition_instance.condition_end) %>
  <%= attribute_table_row_markdown(t("myplaceonline.medical_condition_instances.notes"), medical_condition_instance.notes) %>
<% end %>
<% obj.medical_condition_treatments.each do |x| %>
  <%= table_row_heading(t("myplaceonline.medical_conditions.treatment")) %>
  <%= attribute_table_row_datetime(t("myplaceonline.medical_condition_treatments.treatment_date"), x.treatment_date) %>
  <%= attribute_table_row(t("myplaceonline.medical_condition_treatments.treatment_description"), x.treatment_description) %>
  <%= attribute_table_row_markdown(t("myplaceonline.general.notes"), x.notes) %>
  <%= attribute_table_row_reference(t("myplaceonline.medical_condition_treatments.doctor"), "doctor_path", x.doctor) %>
  <%= attribute_table_row_reference(t("myplaceonline.medical_condition_treatments.location"), "location_path", x.location) %>
<% end %>
<% obj.medical_condition_evaluations.each do |x| %>
  <%= table_row_heading(t("myplaceonline.medical_conditions.evaluation")) %>
  <%= render partial: "medical_condition_evaluations/show", locals: { obj: x } %>
<% end %>
<%=
  render partial: "myplaceonline/pictures", locals: {
    pics: obj.medical_condition_files,
    placeholder: "myplaceonline.identity_files.file"
  }
%>
