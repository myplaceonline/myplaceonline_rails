<%= attribute_table_row_datetime(t("myplaceonline.meals.meal_time"), obj.meal_time) %>
<% obj.meal_foods.each do |wrapper| %>
  <% if wrapper.food %>
      <%= table_row_heading(t("myplaceonline.foods.food")) %>
      <%= attribute_table_row_reference(t("myplaceonline.foods.food_name"), "food_path", wrapper.food) %>
      <%= attribute_table_row(t("myplaceonline.foods.price"), wrapper.food.price) %>
      <%= attribute_table_row(t("myplaceonline.foods.calories"), wrapper.food.calories) %>
      <%= attribute_table_row_food_weight(t("myplaceonline.foods.weight"), wrapper.food.weight, wrapper.food.weight_type) %>
      <%= attribute_table_row(t("myplaceonline.foods.servings"), wrapper.food_servings) %>
      <%= attribute_table_row_markdown(t("myplaceonline.general.notes"), wrapper.food.notes) %>
      <% wrapper.food.food_ingredients.each do |wrapper2| %>
        <%= table_row_heading(t("myplaceonline.foods.food")) %>
        <%= attribute_table_row_reference(t("myplaceonline.foods.food_name"), "food_path", wrapper2.food) %>
        <%= attribute_table_row(t("myplaceonline.foods.price"), wrapper2.food.price) %>
        <%= attribute_table_row(t("myplaceonline.foods.calories"), wrapper2.food.calories) %>
        <%= attribute_table_row_food_weight(t("myplaceonline.foods.weight"), wrapper2.food.weight, wrapper2.food.weight_type) %>
        <%= attribute_table_row_markdown(t("myplaceonline.general.notes"), wrapper2.food.notes) %>
      <% end %>
  <% end %>
<% end %>
<% obj.meal_drinks.each do |wrapper| %>
  <%= table_row_heading(t("myplaceonline.drinks.drink")) %>
  <%= attribute_table_row(t("myplaceonline.drinks.drink_name"), wrapper.drink.drink_name) %>
  <%= attribute_table_row(t("myplaceonline.drinks.price"), wrapper.drink.price) %>
  <%= attribute_table_row(t("myplaceonline.drinks.calories"), wrapper.drink.calories) %>
  <%= attribute_table_row_markdown(t("myplaceonline.general.notes"), wrapper.drink.notes) %>
  <%= attribute_table_row(t("myplaceonline.drinks.servings"), wrapper.drink_servings) %>
<% end %>
<% obj.meal_vitamins.each do |wrapper| %>
  <%= table_row_heading(t("myplaceonline.vitamins.vitamin")) %>
  <%= attribute_table_row(t("myplaceonline.vitamins.vitamin_name"), wrapper.vitamin.vitamin_name) %>
  <%= attribute_table_row_select t("myplaceonline.vitamins.amount_type"), wrapper.vitamin.amount_type, Myp::MEASUREMENTS %>
  <%= attribute_table_row(t("myplaceonline.vitamins.vitamin_amount"), wrapper.vitamin.vitamin_amount) %>
  <%= attribute_table_row_markdown(t("myplaceonline.general.notes"), wrapper.vitamin.notes) %>
  <% wrapper.vitamin.vitamin_ingredients.each do |wrapper2| %>
      <%= table_row_heading(t("myplaceonline.vitamins.vitamin")) %>
      <%= attribute_table_row(t("myplaceonline.vitamins.vitamin_name"), wrapper2.vitamin.vitamin_name) %>
      <%= attribute_table_row_select t("myplaceonline.vitamins.amount_type"), wrapper2.vitamin.amount_type, Myp::MEASUREMENTS %>
      <%= attribute_table_row(t("myplaceonline.vitamins.vitamin_amount"), wrapper2.vitamin.vitamin_amount) %>
      <%= attribute_table_row_markdown(t("myplaceonline.general.notes"), wrapper2.vitamin.notes) %>
  <% end %>
<% end %>
<%= attribute_table_row(t("myplaceonline.meals.price"), obj.price) %>
<%= attribute_table_row(t("myplaceonline.meals.calories"), obj.calories) %>
<%= attribute_table_row_markdown(t("myplaceonline.general.notes"), obj.notes) %>
<%= attribute_table_row_reference(t("myplaceonline.meals.location"), "location_path", obj.location) %>
