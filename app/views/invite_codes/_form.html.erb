<%= myp_text_field(f, :code, "myplaceonline.invite_codes.code", obj.code, autofocus) %>
<%= myp_number_field(f, :max_uses, "myplaceonline.invite_codes.max_uses", obj.max_uses) %>
<%= myp_number_field(f, :current_uses, "myplaceonline.invite_codes.current_uses", obj.current_uses) %>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_TEXT,
    name: :public_name,
    value: obj.public_name,
    placeholder: "myplaceonline.invite_codes.public_name"
  )
%>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_TEXT,
    name: :public_link,
    value: obj.public_link,
    placeholder: "myplaceonline.invite_codes.public_link"
  )
%>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_TEXT_AREA,
    name: :public_description,
    value: obj.public_description,
    placeholder: "myplaceonline.invite_codes.public_description",
    collapsed: obj.public_description.blank?,
  )
%>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_TEXT,
    name: :context_ids,
    value: obj.context_ids,
    placeholder: "myplaceonline.invite_codes.context_ids"
  )
%>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_BOOLEAN,
    name: :hidesuggestion,
    value: obj.hidesuggestion,
    placeholder: "myplaceonline.invite_codes.hidesuggestion"
  )
%>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_BOOLEAN,
    name: :controversial,
    value: obj.controversial,
    placeholder: "myplaceonline.invite_codes.controversial"
  )
%>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_BOOLEAN,
    name: :sexual,
    value: obj.sexual,
    placeholder: "myplaceonline.invite_codes.sexual"
  )
%>
<%=
  render partial: "myplaceonline/select_or_create", locals: {
    f: f,
    name: :website_domain,
    existingobj: obj.website_domain,
    heading: t("myplaceonline.invite_codes.website_domain"),
    existingobjurl: "website_domain_path",
    searchurl: "/website_domains.json?perpage=0",
    edit: edit,
    autofocus: false,
    allow_new: false,
    model: WebsiteDomain,
    expanded_if_nil: true,
    existingcollapsed: false,
  }
%>
<%=
  render partial: "myplaceonline/select_or_create", locals: {
    f: f,
    name: :parent,
    existingobj: obj.get_parent,
    heading: t("myplaceonline.invite_codes.parent"),
    existingobjurl: "invite_code_path",
    searchurl: "/invite_codes.json?perpage=0",
    edit: edit,
    autofocus: false,
    allow_new: false,
    model: InviteCode,
    expanded_if_nil: true,
    existingcollapsed: false,
  }
%>
