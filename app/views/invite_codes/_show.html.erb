<%= attribute_table_row(t("myplaceonline.invite_codes.code"), obj.code) %>
<%= attribute_table_row(t("myplaceonline.invite_codes.max_uses"), obj.max_uses) %>
<%= attribute_table_row(t("myplaceonline.invite_codes.current_uses"), obj.current_uses) %>
<%= data_row(heading: t("myplaceonline.invite_codes.public_name"), content: obj.public_name) %>
<%= data_row(heading: t("myplaceonline.invite_codes.public_link"), content: obj.public_link, url: true, url_external: true, url_external_target_blank: true) %>
<%= data_row(heading: t("myplaceonline.invite_codes.public_description"), content: obj.public_description, markdown: true) %>
<%= data_row(heading: t("myplaceonline.invite_codes.hidesuggestion"), content: obj.hidesuggestion) %>
<%= data_row(heading: t("myplaceonline.invite_codes.website_domain"), content: obj.website_domain) %>
<%= data_row(heading: t("myplaceonline.invite_codes.parent"), content: obj.get_parent) %>
<%= data_row(heading: t("myplaceonline.invite_codes.parent_and_sibling_codes"), content: obj.parent_and_sibling_codes.join(", ")) %>
