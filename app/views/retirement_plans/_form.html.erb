<%= myp_text_field(f, :retirement_plan_name, "myplaceonline.retirement_plans.retirement_plan_name", obj.retirement_plan_name, autofocus) %>
<%= myp_date_field(f, :started, "myplaceonline.retirement_plans.started", obj.started) %>
<%= myp_select(f, :retirement_plan_type, "myplaceonline.retirement_plans.retirement_plan_type", Myp.translate_options(RetirementPlan::RETIREMENT_PLAN_TYPES), obj.retirement_plan_type) %>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_TEXT_AREA,
    name: :notes,
    value: obj.notes,
    placeholder: "myplaceonline.general.notes",
    collapsed: obj.notes.blank?,
    placeholder_prefix: Myp.instance_to_category_human_readable(obj).singularize,
  )
%>
<%=
  render partial: "myplaceonline/select_or_create", locals: {
    f: f,
    name: :company,
    existingobj: obj.company,
    heading: t("myplaceonline.retirement_plans.company"),
    objform: "companies/form",
    existingobjurl: "company_path",
    searchurl: "/companies.json?perpage=0",
    edit: edit,
    autofocus: false
  }
%>
<%=
  render partial: "myplaceonline/select_or_create", locals: {
    f: f,
    name: :periodic_payment,
    existingobj: obj.periodic_payment,
    heading: t("myplaceonline.retirement_plans.periodic_payment"),
    objform: "periodic_payments/form",
    existingobjurl: "periodic_payment_path",
    searchurl: "/periodic_payments.json?perpage=0",
    edit: edit,
    autofocus: false
  }
%>
<%=
  render partial: "myplaceonline/select_or_create", locals: {
    f: f,
    name: :password,
    existingobj: obj.password,
    heading: t("myplaceonline.retirement_plans.password"),
    objform: "passwords/form",
    existingobjurl: "password_path",
    searchurl: "/passwords.json?perpage=0",
    edit: edit,
    autofocus: false
  }
%>
