<%=
  render partial: "myplaceonline/link_and_title", locals: {
    f: f,
    obj: obj,
    autofocus: autofocus,
    link_name: :url,
    link_placeholder: "myplaceonline.feeds.url",
    title_name: :name,
    title_placeholder: "myplaceonline.feeds.name",
    button_text: "myplaceonline.websites.request_title"
  }
%>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_BOOLEAN,
    name: :new_notify,
    value: obj.new_notify,
    placeholder: "myplaceonline.feeds.new_notify"
  )
%>
<%=
  render partial: "myplaceonline/select_or_create", locals: {
    f: f,
    name: :password,
    existingobj: obj.password,
    heading: t("myplaceonline.feeds.password"),
    objform: "passwords/form",
    existingobjurl: "password_path",
    searchurl: "/passwords.json?perpage=0",
    edit: edit,
    autofocus: false
  }
%>
