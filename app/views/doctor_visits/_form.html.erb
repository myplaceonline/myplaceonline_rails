<%= myp_date_field(f, :visit_date, "myplaceonline.doctor_visits.visit_date", obj.visit_date) %>
<%= myp_decimal_field(f, :paid, "myplaceonline.doctor_visits.paid", obj.paid) %>
<%= myp_check_box(f, :physical, "myplaceonline.doctor_visits.physical") %>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_TEXT_AREA,
    name: :notes,
    value: obj.notes,
    placeholder: "myplaceonline.general.notes",
    collapsed: obj.notes.blank?,
    placeholder_prefix: Myp.instance_to_category_human_readable(obj).singularize,
  )
%>
<%=
  render partial: "myplaceonline/select_or_create", locals: {
    f: f,
    name: :doctor,
    existingobj: obj.doctor,
    heading: t("myplaceonline.doctor_visits.doctor"),
    objform: "doctors/form",
    existingobjurl: "doctor_path",
    searchurl: "/doctors.json?perpage=0",
    edit: edit,
    autofocus: false
  }
%>
<%=
  render partial: "myplaceonline/select_or_create", locals: {
    f: f,
    name: :health_insurance,
    existingobj: obj.health_insurance,
    heading: t("myplaceonline.doctor_visits.health_insurance"),
    objform: "health_insurances/form",
    existingobjurl: "health_insurance_path",
    searchurl: "/health_insurances.json?perpage=0",
    edit: edit,
    autofocus: false
  }
%>
<%=
  render partial: "myplaceonline/pictures_form", locals: {
    f: f,
    obj: obj,
    position_field: :position,
    pictures_field: :doctor_visit_files,
    item_placeholder: "myplaceonline.identity_files.file",
    heading: "myplaceonline.identity_files.files",
    addbutton: "myplaceonline.identity_files.add_file",
    addbutton_multi: "myplaceonline.identity_files.add_files",
    deletebutton: "myplaceonline.identity_files.delete_file"
  }
%>
