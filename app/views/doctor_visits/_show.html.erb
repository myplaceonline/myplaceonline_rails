<%= attribute_table_row_date(t("myplaceonline.doctor_visits.visit_date"), obj.visit_date) %>
<%= attribute_table_row_currency(t("myplaceonline.doctor_visits.paid"), obj.paid) %>
<%= attribute_table_row_boolean(t("myplaceonline.doctor_visits.physical"), obj.physical) %>
<%= attribute_table_row_markdown(t("myplaceonline.general.notes"), obj.notes) %>
<%= attribute_table_row_reference(t("myplaceonline.doctor_visits.doctor"), "doctor_path", obj.doctor) %>
<%= attribute_table_row_reference(t("myplaceonline.doctor_visits.health_insurance"), "health_insurance_path", obj.health_insurance) %>
<%=
  render partial: "myplaceonline/pictures", locals: {
    pics: obj.doctor_visit_files,
    placeholder: "myplaceonline.identity_files.file"
  }
%>
