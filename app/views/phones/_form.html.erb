<%= myp_text_field(f, :phone_model_name, "myplaceonline.phones.phone_model_name", obj.phone_model_name, true) %>
<%=
  render partial: "myplaceonline/select_or_create", locals: {
    f: f,
    name: :manufacturer,
    existingobj: obj.manufacturer,
    heading: t("myplaceonline.phones.manufacturer"),
    objform: "companies/form",
    existingobjurl: "company_path",
    searchurl: "/companies.json?perpage=0",
    edit: edit,
    autofocus: false
  }
%>
<%= myp_text_field(f, :phone_number, "myplaceonline.phones.phone_number", obj.phone_number) %>
<%= myp_date_field(f, :purchased, "myplaceonline.phones.purchased", obj.purchased) %>
<%= myp_decimal_field(f, :price, "myplaceonline.phones.price", obj.price) %>
<%= myp_select(f, :operating_system, "myplaceonline.phones.operating_system", Myp.translate_options(Phone::OPERATING_SYSTEMS), obj.operating_system) %>
<%= myp_text_field(f, :operating_system_version, "myplaceonline.phones.operating_system_version", obj.operating_system_version) %>
<%= myp_integer_field(f, :max_resolution_width, "myplaceonline.phones.max_resolution_width", obj.max_resolution_width) %>
<%= myp_integer_field(f, :max_resolution_height, "myplaceonline.phones.max_resolution_height", obj.max_resolution_height) %>
<%= myp_integer_field(f, :ram, "myplaceonline.phones.ram", obj.ram) %>
<%= myp_integer_field(f, :num_cpus, "myplaceonline.phones.num_cpus", obj.num_cpus) %>
<%= myp_integer_field(f, :num_cores_per_cpu, "myplaceonline.phones.num_cores_per_cpu", obj.num_cores_per_cpu) %>
<%= myp_check_box(f, :hyperthreaded, "myplaceonline.phones.hyperthreaded") %>
<%= myp_decimal_field(f, :max_cpu_speed, "myplaceonline.phones.max_cpu_speed", obj.max_cpu_speed) %>
<%= myp_check_box(f, :cdma, "myplaceonline.phones.cdma") %>
<%= myp_check_box(f, :gsm, "myplaceonline.phones.gsm") %>
<%= myp_decimal_field(f, :front_camera_megapixels, "myplaceonline.phones.front_camera_megapixels", obj.front_camera_megapixels) %>
<%= myp_decimal_field(f, :back_camera_megapixels, "myplaceonline.phones.back_camera_megapixels", obj.back_camera_megapixels) %>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_TEXT_AREA,
    name: :notes,
    value: obj.notes,
    placeholder: "myplaceonline.general.notes",
    collapsed: obj.notes.blank?,
    placeholder_prefix: Myp.instance_to_category_human_readable(obj).singularize,
  )
%>
<%=
  render partial: "myplaceonline/select_or_create", locals: {
    f: f,
    name: :password,
    existingobj: obj.password,
    heading: t("myplaceonline.phones.password"),
    objform: "passwords/form",
    existingobjurl: "password_path",
    searchurl: "/passwords.json?perpage=0",
    edit: edit,
    autofocus: false
  }
%>
<%= myp_select(f, :dimensions_type, "myplaceonline.phones.dimensions_type", Myp.translate_options(Myp::DIMENSIONS), obj.dimensions_type) %>
<%= myp_decimal_field(f, :width, "myplaceonline.phones.width", obj.width) %>
<%= myp_decimal_field(f, :height, "myplaceonline.phones.height", obj.height) %>
<%= myp_decimal_field(f, :depth, "myplaceonline.phones.depth", obj.depth) %>
<%= myp_select(f, :weight_type, "myplaceonline.phones.weight_type", Myp.translate_options(Myp::WEIGHTS), obj.weight_type) %>
<%= myp_decimal_field(f, :weight, "myplaceonline.phones.weight", obj.weight) %>
<%=
  render partial: "myplaceonline/pictures_form", locals: {
    f: f,
    obj: obj,
    pictures_field: :phone_files,
    item_placeholder: "myplaceonline.phones.file",
    heading: "myplaceonline.phones.files",
    addbutton: "myplaceonline.phones.add_file",
    addbutton_multi: "myplaceonline.phones.add_files",
    deletebutton: "myplaceonline.phones.delete_file"
  }
%>
