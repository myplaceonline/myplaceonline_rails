<%= attribute_table_row(t("myplaceonline.phones.phone_model_name"), obj.phone_model_name) %>
<%= attribute_table_row_reference(t("myplaceonline.phones.manufacturer"), "company_path", obj.manufacturer) %>
<%= attribute_table_row(t("myplaceonline.phones.phone_number"), obj.phone_number) %>
<%= attribute_table_row_date(t("myplaceonline.phones.purchased"), obj.purchased) %>
<%= attribute_table_row_currency(t("myplaceonline.phones.price"), obj.price) %>
<%= attribute_table_row_select(t("myplaceonline.phones.operating_system"), obj.operating_system, Phone::OPERATING_SYSTEMS) %>
<%= attribute_table_row(t("myplaceonline.phones.operating_system_version"), obj.operating_system_version) %>
<%= attribute_table_row(t("myplaceonline.phones.max_resolution_width"), obj.max_resolution_width) %>
<%= attribute_table_row(t("myplaceonline.phones.max_resolution_height"), obj.max_resolution_height) %>
<%= attribute_table_row(t("myplaceonline.phones.ram"), obj.ram) %>
<%= attribute_table_row(t("myplaceonline.phones.num_cpus"), obj.num_cpus) %>
<%= attribute_table_row(t("myplaceonline.phones.num_cores_per_cpu"), obj.num_cores_per_cpu) %>
<%= attribute_table_row_boolean(t("myplaceonline.phones.hyperthreaded"), obj.hyperthreaded) %>
<%= attribute_table_row(t("myplaceonline.phones.max_cpu_speed"), obj.max_cpu_speed) %>
<%= attribute_table_row_boolean(t("myplaceonline.phones.cdma"), obj.cdma) %>
<%= attribute_table_row_boolean(t("myplaceonline.phones.gsm"), obj.gsm) %>
<%= attribute_table_row(t("myplaceonline.phones.front_camera_megapixels"), obj.front_camera_megapixels) %>
<%= attribute_table_row(t("myplaceonline.phones.back_camera_megapixels"), obj.back_camera_megapixels) %>
<%= attribute_table_row_markdown(t("myplaceonline.general.notes"), obj.notes) %>
<%= attribute_table_row_reference(t("myplaceonline.phones.password"), "password_path", obj.password) %>
<%= attribute_table_row_dimensions(t("myplaceonline.phones.width"), obj.width, obj.dimensions_type) %>
<%= attribute_table_row_dimensions(t("myplaceonline.phones.height"), obj.height, obj.dimensions_type) %>
<%= attribute_table_row_dimensions(t("myplaceonline.phones.depth"), obj.depth, obj.dimensions_type) %>
<%= attribute_table_row_weight(t("myplaceonline.phones.weight"), obj.weight, obj.weight_type) %>
<%=
  render partial: "myplaceonline/pictures", locals: {
      pics: obj.phone_files,
      placeholder: "myplaceonline.phones.file"
  }
%>
