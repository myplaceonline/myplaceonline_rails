<%= myp_date_field(f, :started, "myplaceonline.job_salaries.started", obj.started, autofocus) %>
<%= myp_date_field(f, :ended, "myplaceonline.job_salaries.ended", obj.ended) %>
<%= myp_decimal_field(f, :salary, "myplaceonline.job_salaries.salary", obj.salary) %>
<%= myp_select(f, :salary_period, "myplaceonline.job_salaries.salary_period", Myp.translate_options(Myp::PERIODS), obj.salary_period) %>
<%= myp_decimal_field(f, :hours_per_week, "myplaceonline.jobs.hours_per_week", obj.hours_per_week) %>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_TEXT_AREA,
    name: :notes,
    value: obj.notes,
    placeholder: "myplaceonline.general.notes",
    collapsed: obj.notes.blank?,
    placeholder_prefix: I18n.t("myplaceonline.category.job_salaries").singularize,
  )
%>
<%= myp_text_field(f, :new_title, "myplaceonline.job_salaries.new_title", obj.new_title) %>
