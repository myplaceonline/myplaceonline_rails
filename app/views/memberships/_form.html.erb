<%= myp_text_field f, :name, "myplaceonline.memberships.name", obj.name, autofocus %>
<%= myp_text_field f, :membership_identifier, "myplaceonline.memberships.membership_identifier", obj.membership_identifier %>
<%= myp_date_field f, :start_date, "myplaceonline.memberships.start_date", obj.start_date %>
<%= myp_date_field f, :end_date, "myplaceonline.memberships.end_date", obj.end_date %>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_TEXT_AREA,
    name: :notes,
    value: obj.notes,
    placeholder: "myplaceonline.general.notes",
    collapsed: obj.notes.blank?,
    placeholder_prefix: Myp.instance_to_category_human_readable(obj).singularize,
  )
%>
<%=
  render partial: "myplaceonline/select_or_create", locals: {
    f: f,
    name: :periodic_payment,
    existingobj: obj.periodic_payment,
    heading: t("myplaceonline.memberships.periodic_payment"),
    objform: "periodic_payments/form",
    existingobjurl: "periodic_payment_path",
    searchurl: "/periodic_payments.json?perpage=0",
    edit: edit,
    autofocus: false
  }
%>
<%=
  render partial: "myplaceonline/pictures_form", locals: {
    f: f,
    obj: obj,
    position_field: :position,
    pictures_field: :membership_files,
    item_placeholder: "myplaceonline.memberships.file",
    heading: "myplaceonline.memberships.files",
    addbutton: "myplaceonline.memberships.add_file",
    addbutton_multi: "myplaceonline.memberships.add_files",
    deletebutton: "myplaceonline.memberships.delete_file"
  }
%>
<%=
  render partial: "myplaceonline/select_or_create", locals: {
    f: f,
    name: :password,
    existingobj: obj.password,
    heading: t("myplaceonline.memberships.password"),
    objform: "passwords/form",
    existingobjurl: "password_path",
    searchurl: "/passwords.json?perpage=0",
    edit: edit,
    autofocus: false
  }
%>
