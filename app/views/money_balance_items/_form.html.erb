<%= myp_datetime_field(f, :item_time, "myplaceonline.money_balance_items.item_time", obj.item_time) %>
<%= myp_decimal_field(f, :amount, "myplaceonline.money_balance_items.amount", obj.amount) %>
<%= myp_decimal_field(f, :original_amount, "myplaceonline.money_balance_items.original_amount", obj.original_amount) %>
<%= myp_text_field(f, :money_balance_item_name, "myplaceonline.money_balance_items.money_balance_item_name", obj.money_balance_item_name) %>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_TEXT_AREA,
    name: :notes,
    value: obj.notes,
    placeholder: "myplaceonline.general.notes",
    collapsed: obj.notes.blank?,
    placeholder_prefix: Myp.instance_to_category_human_readable(obj).singularize,
  )
%>
