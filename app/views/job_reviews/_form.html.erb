<%= myp_date_field(f, :review_date, "myplaceonline.job_reviews.review_date", obj.review_date, autofocus) %>
<%= myp_text_field(f, :company_score, "myplaceonline.job_reviews.company_score", obj.company_score) %>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_TEXT_AREA,
    name: :notes,
    value: obj.notes,
    placeholder: "myplaceonline.job_reviews.notes",
    collapsed: obj.notes.blank?,
    placeholder_prefix: Myp.instance_to_category_human_readable(obj).singularize,
  )
%>
<%= myp_text_area_markdown(f, :self_evaluation, "myplaceonline.job_reviews.self_evaluation", obj.self_evaluation) %>
<%=
  render partial: "myplaceonline/select_or_create", locals: {
    f: f,
    name: :contact_attributes,
    existingobj: obj.contact,
    heading: t("myplaceonline.job_reviews.contact"),
    objform: "contacts/form",
    existingobjurl: "contact_path",
    searchurl: "/contacts.json?perpage=0",
    edit: edit,
    autofocus: false
  }
%>
<%=
  render partial: "myplaceonline/pictures_form", locals: {
    f: f,
    obj: obj,
    position_field: :position,
    pictures_field: :job_review_files,
    item_placeholder: "myplaceonline.job_reviews.file",
    heading: "myplaceonline.job_reviews.files",
    addbutton: "myplaceonline.job_reviews.add_file",
    addbutton_multi: "myplaceonline.job_reviews.add_files",
    deletebutton: "myplaceonline.job_reviews.delete_file"
  }
%>
