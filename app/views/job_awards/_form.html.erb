<%= myp_text_field(f, :job_award_description, "myplaceonline.job_awards.job_award_description", obj.job_award_description, autofocus) %>
<%= myp_date_field(f, :job_award_date, "myplaceonline.job_awards.job_award_date", obj.job_award_date) %>
<%= myp_decimal_field(f, :job_award_amount, "myplaceonline.job_awards.job_award_amount", obj.job_award_amount) %>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_TEXT_AREA,
    name: :notes,
    value: obj.notes,
    placeholder: "myplaceonline.general.notes",
    collapsed: obj.notes.blank?,
    placeholder_prefix: Myp.instance_to_category_human_readable(obj).singularize,
  )
%>
