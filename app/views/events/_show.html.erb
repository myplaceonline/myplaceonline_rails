<%= attribute_table_row(t("myplaceonline.events.event_name"), obj.event_name) %>
<%= attribute_table_row_datetime(t("myplaceonline.events.event_time"), obj.event_time) %>
<%= attribute_table_row_datetime(t("myplaceonline.events.event_end_time"), obj.event_end_time) %>
<%= data_row(heading: t("myplaceonline.events.cost"), content: obj.cost, currency: true) %>
<%= attribute_table_row_markdown(t("myplaceonline.general.notes"), obj.notes) %>
<% if !obj.repeat.nil? %>
  <%=
      render partial: 'repeats/show', locals: {
        obj: obj.repeat
      }
    %>
<% end %>
<%= attribute_table_row_reference(t("myplaceonline.events.location"), "location_path", obj.location) %>
<%=
  render partial: "myplaceonline/pictures", locals: {
      pics: obj.event_pictures,
      placeholder: "myplaceonline.events.file"
  }
%>
<% obj.event_contacts.each do |wrapper| %>
  <%= attribute_table_row_reference(t("myplaceonline.events.contact"), "contact_path", wrapper.contact) %>
<% end %>
