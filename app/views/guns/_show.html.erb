<%= attribute_table_row(t("myplaceonline.guns.gun_name"), obj.gun_name) %>
<%= attribute_table_row(t("myplaceonline.guns.gun_model"), obj.gun_model) %>
<%= attribute_table_row(t("myplaceonline.guns.manufacturer_name"), obj.manufacturer_name) %>
<%= attribute_table_row(t("myplaceonline.guns.bullet_caliber"), obj.bullet_caliber) %>
<%= attribute_table_row(t("myplaceonline.guns.max_bullets"), obj.max_bullets) %>
<%= attribute_table_row_currency(t("myplaceonline.guns.price"), obj.price) %>
<%= attribute_table_row_date(t("myplaceonline.guns.purchased"), obj.purchased) %>
<%= attribute_table_row_markdown(t("myplaceonline.general.notes"), obj.notes) %>
<% obj.gun_registrations.each do |x| %>
  <%= table_row_heading(t("myplaceonline.guns.registration")) %>
  <%= render partial: "gun_registrations/show", locals: { obj: x } %>
<% end %>
<%=
  render partial: "myplaceonline/pictures", locals: {
    pics: obj.gun_files,
    placeholder: "myplaceonline.identity_files.file"
  }
%>
