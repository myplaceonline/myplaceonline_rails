  <%= table_row_heading(t("myplaceonline.vehicle_service.services")) %>
  <%= attribute_table_row(t("myplaceonline.vehicle_service.short_description"), service.short_description) %>
  <%= attribute_table_row(t("myplaceonline.vehicle_service.date_due"), Myp.display_date(service.date_due, current_user)) %>
  <%= attribute_table_row(t("myplaceonline.vehicle_service.date_serviced"), Myp.display_date(service.date_serviced, current_user)) %>
  <%= attribute_table_row(t("myplaceonline.vehicle_service.service_location"), service.service_location) %>
  <%= attribute_table_row(t("myplaceonline.vehicle_service.cost"), service.cost) %>
  <%= attribute_table_row(t("myplaceonline.vehicle_service.miles"), service.miles) %>
  <%= attribute_table_row_markdown(t("myplaceonline.vehicle_service.notes"), service.notes) %>
<% end %>
<% obj.vehicle_insurances.each do |insurance| %>
  <%= table_row_heading(t("myplaceonline.vehicle_insurances.insurance")) %>
  <%= attribute_table_row(t("myplaceonline.vehicle_insurances.insurance_name"), insurance.insurance_name) %>
  <%= attribute_table_row_date(t("myplaceonline.vehicle_insurances.date_due"), insurance.started) %>
  <%= attribute_table_row_markdown(t("myplaceonline.vehicle_insurances.notes"), insurance.notes) %>
  <%= attribute_table_row_reference(t("myplaceonline.vehicle_insurances.company"), "company_path", insurance.company) %>
  <%= attribute_table_row_reference(t("myplaceonline.vehicle_insurances.periodic_payment"), "periodic_payment_path", insurance.periodic_payment) %>
<% end %>
<%=
    render partial: 'myplaceonline/pictures', locals: {
      pics: obj.vehicle_pictures,
      placeholder: "myplaceonline.vehicles.picture"
    }
  %>
<% obj.vehicle_warranties.each do |wrapper| %>
  <%= table_row_heading(t("myplaceonline.warranties.warranty")) %>
  <%= attribute_table_row_reference(t("myplaceonline.warrnties.warranty_name"), "warranty_path", wrapper.warranty) %>
  <%= attribute_table_row(t("myplaceonline.warranties.warranty_name"), wrapper.warranty.warranty_name) %>
  <%= attribute_table_row_date(t("myplaceonline.warranties.warranty_start"), wrapper.warranty.warranty_start) %>
  <%= attribute_table_row_date(t("myplaceonline.warranties.warranty_end"), wrapper.warranty.warranty_end) %>
  <%= attribute_table_row(t("myplaceonline.warranties.warranty_condition"), wrapper.warranty.warranty_condition) %>
