<%= attribute_table_row(t("myplaceonline.general.region"), obj.region) %>
<%= attribute_table_row(t("myplaceonline.general.sub_region1"), obj.sub_region1) %>
<%= attribute_table_row_markdown(t("myplaceonline.general.notes"), obj.notes) %>
<%= attribute_table_row t("myplaceonline.vehicles.horsepower"), obj.horsepower %>
<%= attribute_table_row t("myplaceonline.vehicles.trim_name"), obj.trim_name %>
<%= attribute_table_row_select t("myplaceonline.vehicles.engine_type"), obj.engine_type, Vehicle::ENGINE_TYPES %>
<%= attribute_table_row_select t("myplaceonline.vehicles.displacement_type"), obj.displacement_type, Vehicle::DISPLACEMENT_TYPES %>
<%= attribute_table_row t("myplaceonline.vehicles.displacement"), obj.displacement %>
<%= attribute_table_row t("myplaceonline.vehicles.cylinders"), obj.cylinders %>
<%= attribute_table_row t("myplaceonline.vehicles.passenger_seats"), obj.passenger_seats %>
<%= attribute_table_row t( "myplaceonline.vehicles.doors"), obj.doors %>
<%= attribute_table_row_select t("myplaceonline.vehicles.doors_type"), obj.doors_type, Vehicle::DOOR_TYPES %>
<%= attribute_table_row_select t("myplaceonline.vehicles.wheel_drive_type"), obj.wheel_drive_type, Vehicle::DRIVE_TYPES %>
<%= attribute_table_row_select t("myplaceonline.vehicles.dimensions_type"), obj.dimensions_type, Myp::DIMENSIONS %>
<%= attribute_table_row_dimensions t("myplaceonline.vehicles.bed_length"), obj.bed_length, obj.dimensions_type %>
<%= attribute_table_row_dimensions t("myplaceonline.vehicles.height"), obj.height, obj.dimensions_type %>
<% if !obj.height.blank? && !obj.recreational_vehicle.nil? && !obj.recreational_vehicle.exterior_height_over.nil? %>
  <%= attribute_table_row_dimensions t("myplaceonline.vehicles.height_with_rv"), obj.height + obj.recreational_vehicle.exterior_height_over, obj.dimensions_type %>
<% end %>
<%= attribute_table_row_dimensions t("myplaceonline.vehicles.width"), obj.width, obj.dimensions_type %>
<%= attribute_table_row_dimensions t("myplaceonline.vehicles.length"), obj.length, obj.dimensions_type %>
<% if !obj.length.blank? && !obj.recreational_vehicle.nil? && !obj.recreational_vehicle.exterior_length_over.nil? %>
  <%= attribute_table_row_dimensions t("myplaceonline.vehicles.length_with_rv"), obj.length + obj.recreational_vehicle.exterior_length_over, obj.dimensions_type %>
<% end %>
<%= attribute_table_row_dimensions t("myplaceonline.vehicles.wheel_base"), obj.wheel_base, obj.dimensions_type %>
<%= attribute_table_row_dimensions t("myplaceonline.vehicles.ground_clearance"), obj.ground_clearance, obj.dimensions_type %>
<%= attribute_table_row_select t("myplaceonline.vehicles.weight_type"), obj.weight_type, Myp::WEIGHTS %>
<%= attribute_table_row t("myplaceonline.vehicles.gvwr"), obj.gvwr %>
<%= attribute_table_row t("myplaceonline.vehicles.gcwr"), obj.gcwr %>
<%= attribute_table_row t("myplaceonline.vehicles.gawr_front"), obj.gawr_front %>
<%= attribute_table_row t("myplaceonline.vehicles.gawr_rear"), obj.gawr_rear %>
<%= attribute_table_row t("myplaceonline.vehicles.front_axle_details"), obj.front_axle_details %>
<%= attribute_table_row t("myplaceonline.vehicles.front_axle_rating"), obj.front_axle_rating %>
<%= attribute_table_row t("myplaceonline.vehicles.front_suspension_details"), obj.front_suspension_details %>
<%= attribute_table_row t("myplaceonline.vehicles.front_suspension_rating"), obj.front_suspension_rating %>
<%= attribute_table_row t("myplaceonline.vehicles.rear_axle_details"), obj.rear_axle_details %>
<%= attribute_table_row t("myplaceonline.vehicles.rear_axle_rating"), obj.rear_axle_rating %>
<%= attribute_table_row t("myplaceonline.vehicles.rear_suspension_details"), obj.rear_suspension_details %>
<%= attribute_table_row t("myplaceonline.vehicles.rear_suspension_rating"), obj.rear_suspension_rating %>
<%= attribute_table_row t("myplaceonline.vehicles.tire_details"), obj.tire_details %>
<%= attribute_table_row t("myplaceonline.vehicles.tire_rating"), obj.tire_rating %>
<%= attribute_table_row t("myplaceonline.vehicles.tire_diameter"), obj.tire_diameter %>
<%= attribute_table_row t("myplaceonline.vehicles.wheel_details"), obj.wheel_details %>
<%= attribute_table_row t("myplaceonline.vehicles.wheel_rating"), obj.wheel_rating %>
<%= attribute_table_row_select t("myplaceonline.vehicles.wheels_type"), obj.wheels_type, Vehicle::WHEEL_TYPES %>
<%= attribute_table_row_select t("myplaceonline.vehicles.fuel_tank_capacity_type"), obj.fuel_tank_capacity_type, Myp::LIQUID_CAPACITY %>
<%= attribute_table_row t("myplaceonline.vehicles.fuel_tank_capacity"), obj.fuel_tank_capacity %>
<%= attribute_table_row t("myplaceonline.vehicles.wet_weight_front"), obj.wet_weight_front %>
<%= attribute_table_row t("myplaceonline.vehicles.wet_weight_rear"), obj.wet_weight_rear %>
<% if !obj.wet_weight_front.blank? && !obj.wet_weight_rear.blank? %>
  <%= attribute_table_row t("myplaceonline.vehicles.wet_weight"), obj.wet_weight_front + obj.wet_weight_rear %>
  <% if !obj.recreational_vehicle.nil? && !obj.recreational_vehicle.wet_weight.nil? %>
      <%= attribute_table_row t("myplaceonline.vehicles.wet_weight_with_rv"), obj.wet_weight_front + obj.wet_weight_rear + obj.recreational_vehicle.wet_weight %>
  <% end %>
<% end %>
<%= attribute_table_row t("myplaceonline.vehicles.tailgate_weight"), obj.tailgate_weight %>
<% if !obj.gvwr.blank? && !obj.wet_weight_front.blank? && !obj.wet_weight_rear.blank? %>
  <%= attribute_table_row t("myplaceonline.vehicles.max_payload"), obj.gvwr - obj.wet_weight_front - obj.wet_weight_rear %>
  <% if !obj.tailgate_weight.blank? %>
      <%= attribute_table_row t("myplaceonline.vehicles.max_payload_wo_tailgate"), obj.gvwr - obj.wet_weight_front - obj.wet_weight_rear + obj.tailgate_weight %>
      <% if !obj.recreational_vehicle.nil? && !obj.recreational_vehicle.wet_weight.nil? %>
        <%= attribute_table_row t("myplaceonline.vehicles.max_payload_wo_tailgate_with_rv"), obj.gvwr - obj.wet_weight_front - obj.wet_weight_rear + obj.tailgate_weight - obj.recreational_vehicle.wet_weight %>
        <%
          last_weight = current_user.current_identity.last_weight
          if !last_weight.nil?
        %>
          <%= attribute_table_row t("myplaceonline.vehicles.max_payload_wo_tailgate_with_rv_with_driver"), obj.gvwr - obj.wet_weight_front - obj.wet_weight_rear + obj.tailgate_weight - obj.recreational_vehicle.wet_weight - last_weight.amount %>
        <% end %>
      <% end %>
  <% end %>
<% end %>
<%= attribute_table_row_reference(t("myplaceonline.vehicles.recreational_vehicle"), "recreational_vehicle_path", obj.recreational_vehicle) %>
<% obj.vehicle_loans.each do |loanwrapper| %>
  <%= table_row_heading(t("myplaceonline.loans.loan")) %>
  <%= attribute_table_row(t("myplaceonline.loans.lender"), loanwrapper.loan.lender) %>
  <%= attribute_table_row(t("myplaceonline.loans.amount"), loanwrapper.loan.amount) %>
  <%= attribute_table_row(t("myplaceonline.loans.monthly_payment"), loanwrapper.loan.monthly_payment) %>
  <%= attribute_table_row(t("myplaceonline.loans.start"), Myp.display_date(loanwrapper.loan.start, current_user)) %>
  <%= attribute_table_row(t("myplaceonline.loans.paid_off"), Myp.display_date(loanwrapper.loan.paid_off, current_user)) %>
<% end %>
<% obj.vehicle_insurances.each do |insurance| %>
  <%= table_row_heading(t("myplaceonline.vehicle_insurances.insurance")) %>
  <%= attribute_table_row(t("myplaceonline.vehicle_insurances.insurance_name"), insurance.insurance_name) %>
  <%= attribute_table_row_date(t("myplaceonline.vehicle_insurances.date_due"), insurance.started) %>
  <%= attribute_table_row_markdown(t("myplaceonline.general.notes"), insurance.notes) %>
  <%= attribute_table_row_reference(t("myplaceonline.vehicle_insurances.company"), "company_path", insurance.company) %>
  <%= attribute_table_row_reference(t("myplaceonline.vehicle_insurances.periodic_payment"), "periodic_payment_path", insurance.periodic_payment) %>
<% end %>
<%=
  render partial: "myplaceonline/pictures", locals: {
      pics: obj.vehicle_pictures,
      placeholder: "myplaceonline.vehicles.picture"
  }
%>
<% obj.vehicle_warranties.each do |wrapper| %>
  <%= table_row_heading(t("myplaceonline.warranties.warranty")) %>
  <%= attribute_table_row_reference(t("myplaceonline.warrnties.warranty_name"), "warranty_path", wrapper.warranty) %>
  <%= attribute_table_row(t("myplaceonline.warranties.warranty_name"), wrapper.warranty.warranty_name) %>
  <%= attribute_table_row_date(t("myplaceonline.warranties.warranty_start"), wrapper.warranty.warranty_start) %>
  <%= attribute_table_row_date(t("myplaceonline.warranties.warranty_end"), wrapper.warranty.warranty_end) %>
  <%= attribute_table_row(t("myplaceonline.warranties.warranty_condition"), wrapper.warranty.warranty_condition) %>
  <%= attribute_table_row_markdown(t("myplaceonline.general.notes"), wrapper.warranty.notes) %>
<% end %>
<% obj.vehicle_registrations.each do |x| %>
  <%= table_row_heading(t("myplaceonline.vehicles.registration")) %>
  <%= render partial: "vehicle_registrations/show", locals: { obj: x } %>
<% end %>
