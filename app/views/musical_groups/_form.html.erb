<%= myp_text_field(f, :musical_group_name, "myplaceonline.musical_groups.musical_group_name", obj.musical_group_name, autofocus) %>
<%= myp_text_field(f, :musical_genre, "myplaceonline.musical_groups.musical_genre", obj.musical_genre, remote_autocomplete_model: obj.class) %>
<%= myp_check_box(f, :is_listened_to, "myplaceonline.musical_groups.is_listened_to") %>
<%= myp_check_box(f, :awesome, "myplaceonline.musical_groups.awesome") %>
<%= myp_check_box(f, :secret, "myplaceonline.musical_groups.secret") %>
<%= myp_select(f, :rating, "myplaceonline.musical_groups.rating", Myp.translate_options(Myp::RATINGS), obj.rating) %>
<%=
  input_field(
    form: f,
    type: Myp::FIELD_TEXT_AREA,
    name: :notes,
    value: obj.notes,
    placeholder: "myplaceonline.general.notes",
    collapsed: obj.notes.blank?,
    placeholder_prefix: Myp.instance_to_category_human_readable(obj).singularize,
  )
%>
